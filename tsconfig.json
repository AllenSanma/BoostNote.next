{
  "compilerOptions": {
    "target": "es2017",
    "module": "es6",
    "allowJs": false,
    "jsx": "react",
    "baseUrl": "./src",
    "outDir":"build",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "inlineSourceMap": true,
    "noImplicitAny": true,
    "experimentalDecorators": true
  },
  "include": [
    "src/**/*"
  ]
}
